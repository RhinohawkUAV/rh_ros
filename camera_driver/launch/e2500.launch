<launch>
  <node pkg="camera_driver" type="e2500_service_client.py" name="camera_pump"     ns="e2500"/>
  <node pkg="camera_driver" type="e2500.py"                name="webcam"          ns="e2500"/>
  <node pkg="image_proc"    type="image_proc"              name="image_proc"      ns="e2500"/>
  <node pkg="camera_driver" type="people_detector"         name="people_detector" ns="e2500">
    <remap from="image" to="/e2500/image_rect"/>
    <remap from="people" to="/e2500/image_people"/>
  </node>
</launch>
